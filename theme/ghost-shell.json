            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": [
                "meta.diff"
            ],
            "settings": {
                "foreground": "#d5d5d5"
            }
        },
        {
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#f44747",
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "markup.bold",
                "strong"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#569cd6"
            }
